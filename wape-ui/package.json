{
  "name": "wape-ui",
  "version": "0.1.2",
  "description": "用于学习组件库开发的项目",
  "main": "dist/wape-ui.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./build/webpack.demo.js",
    "build:docs": "rimraf docs && webpack --config ./build/webpack.demo.pro.js",
    "build:dev": "rimraf dist/wape-ui.js && webpack --config ./build/webpack.pro.dev.js",
    "build:pro": "rimraf dist/wape-ui.min.js && webpack --config ./build/webpack.pro.js",
    "build:all": "rimraf dist && npm run build:dev && npm run build:pro"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq986063761/wape-ui.git"
  },
  "keywords": [
    "vue",
    "ui"
  ],
  "author": "Wan Peng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qq986063761/wape-ui/issues"
  },
  "homepage": "https://github.com/qq986063761/wape-ui#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^4.2.1",
    "html-webpack-plugin": "^4.3.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.1",
    "vue": "^2.7.14",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.4.3",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
